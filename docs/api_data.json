  {
    "type": "get",
    "url": "/anagrams/:word",
    "title": "Get the anagrams for a given word.",
    "name": "GetAnagrams",
    "group": "Anagram",
    "parameter": {
      "fields": {
        "Parameter": [
          {
            "group": "Parameter",
            "type": "Number",
            "optional": false,
            "field": "word",
            "description": "<p>Word to search for.</p> "
          },
          {
            "group": "Parameter",
            "type": "Number",
            "optional": true,
            "field": "limit",
            "description": "<p>Optional Max number of anagrams to return.</p> "
          }
        ]
      }
    },
    "success": {
      "fields": {
        "200": [
          {
            "group": "200",
            "type": "Array",
            "optional": false,
            "field": "anagrams",
            "description": "<p>Array of anagrams that match the given word.</p> "
          }
        ]
      },
      "examples": [
        {
          "title": "HTTP/1.1 200 OK",
          "content": " HTTP/1.1 200 OK\n {\n   anagrams: [\"ared\",\"daer\",\"dare\",\"dear\"]\n }",
          "type": "json"
        }
      ]
    },
    "version": "0.0.0",
